<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.upc.base.mapper.EquipStateMapper">
    <resultMap id="BaseResultMap" type="EquipState">
        <id column="id" property="id" jdbcType="VARCHAR"/>
        <result column="is_open" property="isOpen"/>
        <result column="online" property="online"/>
        <result column="water_usage" property="waterUsage"/>
        <result column="equip_state" property="equipState"/>
        <result column="power_low" property="powerLow"/>
    </resultMap>

    <sql id="baseColumn">
        id,is_open,online,water_usage,equip_state,power_low
    </sql>



</mapper>